{
  "name": "nifi-cuioss-ui",
  "version": "1.0.0",
  "description": "UI components for NiFi MultiIssuerJWTTokenAuthenticator",
  "scripts": {
    "e2e:test": "cypress run",
    "e2e:open": "cypress open",
    "e2e:test:chrome": "cypress run --browser chrome",
    "e2e:test:firefox": "cypress run --browser firefox",
    "e2e:test:edge": "cypress run --browser edge",
    "generate-tokens": "node scripts/generate-test-tokens.js",
    "analyze-console-errors": "node scripts/analyze-console-errors.js",
    "validate-console": "cypress run --env checkConsoleErrors=true",
    "test:console-only": "cypress run --spec cypress/integration/console/**/*.spec.js"
  },
  "dependencies": {
    // Dependencies omitted for brevity
  },
  "devDependencies": {
    "cypress": "^12.3.0",
    // Other dev dependencies omitted for brevity
  }
}
