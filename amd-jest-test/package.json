{
  "name": "amd-jest-test",
  "version": "1.0.0",
  "description": "Minimal example for AMD, Jest, and Babel",
  "main": "src/myModule.js",
  "scripts": {
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-modules-amd": "^7.23.3",
    "@babel/preset-env": "^7.23.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-amd-to-commonjs": "^1.6.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/__tests__/**/*.js"
    ]
  }
}
